if (requestADemo !== null) {
	requestADemo.addEventListener('wpcf7mailsent', function(event) {
		var email = ''
		var shouldSubscribe = false
		var inputs = event.detail.inputs
		var query = ''
		
		for (var i = 0; i < inputs.length; i++) {
			var input = inputs[i]

			if (input.name === 'firstname' || input.name === 'lastname' || input.name === 'email' || input.name === 'company' || input.name === 'phone') {
				if (query === '') {
					query += '?' + input.name + '=' + encodeURI(input.value)
				} else {
					query += '&' + input.name + '=' + encodeURI(input.value)
				}
			}
			if (input.name === 'email') {
				email = encodeURI(input.value)
			}
			if (input.name === 'mailchimp-acceptance') {
				shouldSubscribe = true
			}
		}

		jQuery.ajax({
			url: 'https://www.smartup.io/hubspot' + query,
			type: 'get',
			success: function (data) {}
		});
		
		if (shouldSubscribe === true) {
			jQuery.ajax({
				url: 'https://www.smartup.io/mailchimp?email=' + email,
				type: 'get',
				success: function (data) {}
			});
		}
	}, false)
}

window.addEventListener('load', function() {
	var word = document.getElementById('word-cycle');
	var wrd = jQuery(word);
	
	if (word !== null) {
		var index = 0;
		var words = ['learning', 'customers', 'business', 'teams'];
		window.setInterval(function() {
			wrd.fadeOut(250, function(){ 
				word.innerText = words[index];
			}).fadeIn();
			
			index = index === (words.length - 1) ? 0 : index + 1;
		}, 3000);
	}
